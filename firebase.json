{
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint"
    ],
    "source": "functions"
  },
  "hosting": {
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "engine": {"node":"8"},
    "rewrites": [
      {
        "source": "**",
        "function": "app"
      }
    ]
  }
}
